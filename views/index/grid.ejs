<%
if(!Array.isArray(gridData)){
%>
<div class="grid_1_of_3 images_1_of_3">
    <div class="grid3_txt">
        <h3><%=gridData.grid3Txt%></h3>
    </div>
    <div class="clear"></div>
    <p><%=gridData.grid3Detail%></p>
</div>
<%
}else{
gridData.forEach(function(data){
%>
<div class="grid_1_of_3 images_1_of_3">
    <div class="grid3_txt">
        <h3><%=data.grid3Txt%></h3>
    </div>
    <div class="clear"></div>
    <p><%=data.grid3Detail%></p>
</div>
<%
});
};
%>
<div class="clear"></div>